<div class="log">
	<h1> Sign Up Page</h1>
	<%= form_tag "/users", method: :post do %>
		<div>
		First Name: <input type="text" name="first_name" value="<%= @user.first_name %>"%>
	<br>
		Last Name: <input type="text" name="last_name" value="<%= @user.last_name %>"%>
	<br>
		Email: <input type="email" name="email" value="<%= @user.email %>"%>
	<br>
		Password: <input type="password" name="password" value="<%= @user.password %>" %>
	<br>
		Password Confirmation: <input type="password" name="password_confirmation" value="<%= @user.password_digest %>"%>
	<br>
		<input type="hidden" name="admin" value="false" %>
		<input type="submit" name="Create Account" %>
		</div>
	<% end %>
</div>
<%= button_to "Home", "/rabbitsburgers", method: :get %>